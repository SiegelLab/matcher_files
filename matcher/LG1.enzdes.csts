# Chemical constraints for bagel with 4-nitrophenyl-beta-glucopyranoside
# Alex Carlin, Siegel Lab, UCD
#mod sjb 5-31-15 for matching
# Glu 353 to LG1

CST::BEGIN
  TEMPLATE::    ATOM_MAP:   1     atom_name: C5 O2 C4
  TEMPLATE::    ATOM_MAP:   1     residue3: LG1
  TEMPLATE::    ATOM_MAP:   2     atom_name: OE2 CD CG
  TEMPLATE::    ATOM_MAP:   2     residue1: E 
  CONSTRAINT::  distanceAB: 2.0   0.55   500.0   1    0
  CONSTRAINT::  angle_A:    180.0 25.0   500.0   360  1
  CONSTRAINT::  angle_B:    120.0 25.0   500.0   360  1
  CONSTRAINT::  torsion_B:  180.0 25.0  500.0   360   1
  CONSTRAINT::  torsion_AB:  180.0 25.0  10.0   360   0
  CONSTRAINT::  torsion_A:  -40.0 25.0  10.0   360    0  #this last column changes the number of samples within the cst limits
# Dont need these lines for cst 1 
#  ALGORITHM_INFO:: match 
#  IGNORE_UPSTREAM_PROTON_CHI #ignore upstream (lig) proton chi
#  ALGORITHM_INFO::END

CST::END

# Glu 164 to LG1

CST::BEGIN

  TEMPLATE::    ATOM_MAP:   1     atom_name:  O2 C7 C9
  TEMPLATE::    ATOM_MAP:   1     residue3:   LG1
  TEMPLATE::    ATOM_MAP:   2     atom_name:  OE2 CD CG
  TEMPLATE::    ATOM_MAP:   2     residue1:   E
  CONSTRAINT::  distanceAB: 2.75  0.55        500.0   0     # 1  #change to non covalent, proton donor      
  CONSTRAINT::  angle_A:    120.0 20.0         500.0   360  # 1
  CONSTRAINT::  angle_B:    90.0 20.0         500.0   360  # 1
  CONSTRAINT::  torsion_AB:  30.0 25.0        500.0   180   # 1 shouldnt' be used in the secondardy matcher anyways
  CONSTRAINT::  torsion_B:  -180.0 25.0        500.0   180   # 1 shouldnt' be used in the secondardy matcher anyways
  CONSTRAINT::  torsion_A:  60.0 25.0        500.0   180   # 1 shouldnt' be used in the secondardy matcher anyways

  ALGORITHM_INFO:: match 
  SECONDARY_MATCH: DOWNSTREAM
  ALGORITHM_INFO::END


CST::END

# Tyr 295 to glu 

